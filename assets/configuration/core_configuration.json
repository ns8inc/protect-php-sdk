{
  "version": "0.0.1",
  "default_environment": "testing",
  "logging": {
    "additional_info": {
      "category": "sdk",
      "integration_type": "php"
    },
    "file": {
      "enabled": true,
      "relative_path": "logs/ns8.log",
      "log_level": 100
    },
    "api": {
      "enabled": true,
      "log_level": 100
    },
    "record_all_http_calls": true
  },
  "production": {
    "authorization": {
      "access_token": "",
      "auth_user": ""
    },
    "urls": {
      "client_url": "https://protect-client.ns8.com",
      "api_url": "https://protect.ns8.com"
    }
  },
  "testing": {
    "authorization": {
      "access_token": "",
      "auth_user": ""
    },
    "urls": {
      "client_url": "https://test-protect-client.ns8.com",
      "api_url": "https://test-protect.ns8.com"
    }
  }
}
